{
  "name": "v-clappr",
  "description": "Vue.js wrapper for Clappr",
  "version": "1.1.1",
  "author": {
    "name": "Vinayak Kulkarni",
    "email": "vinayak.kulkarni@live.com",
    "url": "https://vinayakkulkarni.dev"
  },
  "private": false,
  "main": "dist/v-clappr.cjs.js",
  "module": "dist/v-clappr.esm.js",
  "unpkg": "dist/v-clappr.js",
  "jsdelivr": "dist/v-clappr.js",
  "cdn": "dist/v-clappr.js",
  "files": [
    "dist"
  ],
  "keywords": [
    "Vue.js",
    "V-Clappr",
    "Clappr",
    "Vue Component",
    "Vue-Clappr",
    "Vue2-Clappr"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vinayakkulkarni/v-clappr.git"
  },
  "scripts": {
    "test": "ava",
    "test:snapshot": "ava --update-snapshots",
    "build": "npm run bundle && npm run bundle:min",
    "bundle": "rollup -c build/rollup.config.js",
    "bundle:min": "rollup -c build/rollup.min.config.js",
    "format": "prettier --write 'src/**/*.{js,vue}'",
    "lint": "eslint 'src/**/*.{js,vue}'",
    "lintfix": "eslint --fix 'src/**/*.{js,vue}'",
    "release": "shipjs trigger"
  },
  "dependencies": {
    "clappr": "^0.3.12"
  },
  "devDependencies": {
    "@ava/babel": "^1.0.1",
    "@babel/core": "^7.10.1",
    "@babel/preset-env": "^7.10.1",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@vue/test-utils": "^1.0.3",
    "ava": "^3.8.2",
    "babel-eslint": "^10.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "browser-env": "^3.3.0",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-config-vue": "^2.0.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^7.0.1",
    "husky": "^4.2.5",
    "jsdom-global": "^3.0.2",
    "prettier": "^2.0.5",
    "require-extension-hooks": "^0.3.3",
    "require-extension-hooks-babel": "^1.0.0",
    "require-extension-hooks-vue": "^3.0.0",
    "rollup": "^2.11.2",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-terser": "^7.0.0",
    "rollup-plugin-vue": "^5.1.6",
    "shipjs": "^0.23.0",
    "tap-spec": "^5.0.0",
    "vue": "^2.6.11",
    "vue-template-compiler": "^2.6.11"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vinayakkulkarni/v-clappr/issues"
  },
  "homepage": "https://github.com/vinayakkulkarni/v-clappr#readme"
}
